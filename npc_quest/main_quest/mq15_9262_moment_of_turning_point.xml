   <talk_paragraph index="n836_velskud-2-e" image="" itemindex="" portrait_left="" portrait_right="" center_image="">
      <talk_answer link_index="n836_velskud-2-f" mid="94" select_type="0"><![CDATA[<html>...?</html>]]></talk_answer>
