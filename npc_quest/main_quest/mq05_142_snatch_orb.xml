   <talk_paragraph index="n012_sorceress_master_cynthia-accepting" image="QN_Cl_Leonardo.dds" itemindex="">
      <talk_answer link_index="n012_sorceress_master_cynthia-accepting-acceptted" mid="58"><![CDATA[<html>Okay.</html>]]></talk_answer>
